# sshmitm completion                                       -*- shell-script -*-

_comp_cmd_sshmitm()
{
    local cur prev words cword comp_args
    _comp_initialize -- "$@" || return

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '$(_parse_usage "$1")' -- "$cur"))
    else
        _known_hosts_real -- "$cur"
    fi

} &&
    complete -F _comp_cmd_sshmitm sshmitm

# ex: filetype=sh
