# isql completion                                          -*- shell-script -*-
# by Victor Bogado da Silva Lins <victor@bogado.net>

_isql()
{
    local cur prev words cword
    _comp_initialize || return

    [[ -f $ODBCINI ]] &&
        COMPREPLY=($(command grep "\[$cur" "$ODBCINI" | tr -d \[\]))
} &&
    complete -F _isql isql

# ex: filetype=sh
